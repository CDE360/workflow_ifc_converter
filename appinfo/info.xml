	<namespace>WorkflowIFCConverter</namespace>
	<types>
		<filesystem/>
	</types>
	<category>files</category>
	<category>tools</category>
	<category>organization</category>
	<category>workflow</category>
	<website>https://github.com/CDE360/workflow_ifc_converter</website>
